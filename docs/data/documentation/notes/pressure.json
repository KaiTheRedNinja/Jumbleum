  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Pressure",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Density and fluid pressure",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Pressure",
          "level" : 2,
          "text" : "Pressure",
          "type" : "heading"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Pressure is defined as the ",
                  "type" : "text"
                },
                {
                  "inlineContent" : [
                    {
                      "text" : "force acting per unit area",
                      "type" : "text"
                    }
                  ],
                  "type" : "strong"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Definition",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "text" : "Pressure is a scalar quantity, and is measured in pascals (Pa). It is given by the formula:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\text{p} = \\frac{F}{A}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "where",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "p",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the pressure in pascals (Pa)",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "F",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the force in newtons (N)",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "A",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the area in square metres (m²)",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "inlineContent" : [
            {
              "text" : "1 Pa = 1 N\/m²",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Small note about action-reaction pairs when it comes to weight and normal contact force:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Weight and NCF are NOT action-reaction pairs, as they act on the same object",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "CF by the surface and CF on the surface are action-reaction pairs.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Pressure-in-Hydraulic-Systems",
          "level" : 2,
          "text" : "Pressure in Hydraulic Systems",
          "type" : "heading"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Pascal’s principle: When pressure is applied to an enclosed incompressible",
                  "type" : "text"
                },
                {
                  "text" : " ",
                  "type" : "text"
                },
                {
                  "text" : "liquid, the pressure is transmitted equally to all other parts of the liquid.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Definition",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "Diagram_Pressure_Hydraulics",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "The force Fx is exerted on piston 1. The pressure exerted is defined by:",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "code" : [
            "p_x = \\frac{F_x}{A_x}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "This pressure is transmitted equally to every part of the liquid, including to point Y.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "A force, FY, is applied onto the base of piston 2.",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Thus,",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "code" : [
            "p_x = p_y",
            "",
            "\\frac{F_x}{A_x} = \\frac{F_y}{A_y}",
            "",
            "\\frac{F_x}{F_y} = \\frac{A_x}{A_y}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Since liquid is incompressible, the volume of liquid displaced by piston 1 is equal to the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "volume of liquid displaced by piston 2. Thus,",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "V_x = V_y",
            "A_x \\cdot d_x = A_y \\cdot d_y"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "By conservation of energy, the work done on piston 1 is equal to the work done by piston 2.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "WD_x = WD_y",
            "F_x \\cdot d_x = F_y \\cdot d_y"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "In summary,",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\frac{F_x}{F_y} = \\frac{A_x}{A_y} = \\frac{d_y}{d_x}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "anchor" : "Liquid-Pressure",
          "level" : 2,
          "text" : "Liquid Pressure",
          "type" : "heading"
        },
        {
          "content" : [
            {
              "inlineContent" : [
                {
                  "text" : "Liquid pressure is the pressure exerted by a liquid at a certain depth.",
                  "type" : "text"
                }
              ],
              "type" : "paragraph"
            }
          ],
          "name" : "Definition",
          "style" : "note",
          "type" : "aside"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "Diagram_Pressure_Liquid",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "The pressure exerted by a liquid is given by:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "p = \\rho g h"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "where",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "p",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the pressure in pascals (Pa)",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "ρ",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the density of the liquid in kg\/m³",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "code" : "g",
                      "type" : "codeVoice"
                    },
                    {
                      "text" : " is the acceleration due to gravity in m\/s²",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "inlineContent" : [
            {
              "text" : "What this means is that pressure exerted at a constant depth and density of liquid is:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Independent of the shape of the container (even if it goes sideways or whatever)",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "Independent of the area of the container",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        },
        {
          "anchor" : "Atmospheric-Pressure",
          "level" : 2,
          "text" : "Atmospheric Pressure",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Atmospheric pressure is the pressure exerted by the atmosphere. At sea level, it is",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "approximately 101 kPa, or 1 atm.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Note that when calculating pressure, you MUST account for air pressure, especially if",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "the container is open. For example, ten meters underwater is not 10m of water pressure,",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "its 10m of water pressure PLUS 1 atm of air pressure. A general rule of thumb is that",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "10m of water is approximately 1 atm of pressure.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Air pressure is not linear, so you cannot use the liquid pressure formula to calculate",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "air pressure.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Measuring-Pressure",
          "level" : 2,
          "text" : "Measuring Pressure",
          "type" : "heading"
        },
        {
          "anchor" : "Barometer",
          "level" : 3,
          "text" : "Barometer",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "A barometer is a device used to measure atmospheric pressure. It works by balancing the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "pressure of the atmosphere with the pressure of a column of mercury.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "Pressure can be measured in terms of the height of the mercury column, in mmHg. It can",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "be converted to pascals by using the liquid pressure formula:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "p = \\rho g"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "where ",
              "type" : "text"
            },
            {
              "code" : "ρ",
              "type" : "codeVoice"
            },
            {
              "text" : " is the density of mercury (aproxx 13600 kg\/m³, usually provided by the question)",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "and ",
              "type" : "text"
            },
            {
              "code" : "g",
              "type" : "codeVoice"
            },
            {
              "text" : " is the acceleration due to gravity.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "anchor" : "Barometer",
          "level" : 3,
          "text" : "Barometer",
          "type" : "heading"
        },
        {
          "inlineContent" : [
            {
              "text" : "Similar to a manometer, except that it measures the pressure of a gas in a container",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "relative to atmospheric pressure. It works by balancing the pressure of the gas with the",
              "type" : "text"
            },
            {
              "text" : " ",
              "type" : "text"
            },
            {
              "text" : "pressure of a column of mercury.",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "identifier" : "Diagram_Pressure_Manometer",
              "type" : "image"
            }
          ],
          "type" : "paragraph"
        },
        {
          "inlineContent" : [
            {
              "text" : "p0 is the atmospheric pressure",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        }
      ],
      "kind" : "content"
    }
  ],
