  "primaryContentSections" : [
    {
      "content" : [
        {
          "anchor" : "Overview",
          "level" : 2,
          "text" : "Overview",
          "type" : "heading"
        },
        {
          "items" : [
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "symmetry properties of circles:",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                },
                {
                  "items" : [
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "equal chords are equidistant from the centre",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "the perpendicular bisector of a chord passes through the centre",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "tangents from an external point are equal in length",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "the line joining an external point to the centre of the circle bisects the angle between the tangents",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    }
                  ],
                  "type" : "unorderedList"
                }
              ]
            },
            {
              "content" : [
                {
                  "inlineContent" : [
                    {
                      "text" : "angle properties of circles:",
                      "type" : "text"
                    }
                  ],
                  "type" : "paragraph"
                },
                {
                  "items" : [
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "angle in a semicircle is a right angle",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "angle between tangent and radius of a circle is a right angle",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "angle at the centre is twice the angle at the circumference",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "angles in the same segment are equal",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    },
                    {
                      "content" : [
                        {
                          "inlineContent" : [
                            {
                              "text" : "angles in opposite segments are supplementary",
                              "type" : "text"
                            }
                          ],
                          "type" : "paragraph"
                        }
                      ]
                    }
                  ],
                  "type" : "unorderedList"
                }
              ]
            }
          ],
          "type" : "unorderedList"
        }
      ],
      "kind" : "content"
    }
