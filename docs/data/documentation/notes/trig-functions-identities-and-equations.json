        },
        {
          "inlineContent" : [
            {
              "text" : "Quotient identity:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\tan(\\theta) \\equiv \\frac{sin(\\theta)}{cos(\\theta)}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Reciprocal identities:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\cosec(\\theta) \\equiv \\frac{1}{\\sin(\\theta)}",
            "\\sec(\\theta) \\equiv \\frac{1}{\\cos(\\theta)}",
            "\\cot(\\theta) \\equiv \\frac{1}{\\tan(\\theta)}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Pythagoras identity:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\sin^2(\\theta) + \\cos^2(\\theta) \\equiv 1",
            "\\tan^2(\\theta) + 1 \\equiv \\sec^2(\\theta)",
            "1 + \\cot^2(\\theta) \\equiv \\cosec^2(\\theta)"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Addition formulae:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\sin(A \\pm B) = \\sin(A)\\cos(B) \\pm \\cos(A)\\sin(B)",
            "\\cos(A \\pm B) = \\cos(A)\\cos(B) \\mp \\sin(A)\\sin(B)",
            "",
            "\\tan(A \\pm B) = \\frac{\\tan(A) \\pm \\tan(B)}{1 \\mp \\tan(A)\\tan(B)}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Double angle formulae:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\sin(2A) = 2 \\sin(A)\\cos(A)",
            "\\cos(2A) = \\cos^2(A) - \\sin^2(A) ",
            "\\quad = 2\\cos^2(A) - 1 ",
            "\\quad = 1 - 2\\sin^2(A)",
            "",
            "\\tan(2A) = \\frac{2\\tan(A)}{1 - \\tan^2(A)}"
          ],
          "syntax" : "math",
          "type" : "codeListing"
        },
        {
          "inlineContent" : [
            {
              "text" : "Triple angle formulae:",
              "type" : "text"
            }
          ],
          "type" : "paragraph"
        },
        {
          "code" : [
            "\\cos3A = 4 \\cos^3 A - 3\\cos A",
            "\\sin 3A = 3\\sin A - 4\\sin^3 A",
            "",
